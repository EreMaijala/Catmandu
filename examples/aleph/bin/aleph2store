#!/bin/bash

if [ "$1" == "" ]; then
 echo "usage: $0 file"
 exit 1
fi

#PERL="/usr/bin/perl -d:NYTProf"
PERL="/usr/bin/perl"
CATMANDU=${HOME}/Dev/perl/Catmandu
PROJ=${CATMANDU}/examples/aleph

${PERL} ${CATMANDU}/bin/catmandu import \
     -v \
     -I Aleph -i map=${PROJ}/data/aleph.map \
     --fix=${PROJ}/data/aleph.fix \
     -s path=${PROJ}/data/aleph.db $1
