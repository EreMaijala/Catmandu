#!/bin/bash

PERL="/usr/bin/perl"
CATMANDU=${HOME}/Dev/perl/Catmandu
PROJ=${CATMANDU}/examples/aleph

if [ "$1" == "" ]; then
  echo "usage: $0 file"
  exit 1
fi

OUTFILE="$1.$$"

zcat $1 | ${PERL} ${PROJ}/bin/marc_aleph --fix "add_field('920','','',a=>'ejournal')" /dev/stdin | gzip > $OUTFILE

mv $OUTFILE $1

