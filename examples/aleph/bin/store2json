#!/bin/bash

PERL="/usr/bin/perl"
CATMANDU=${HOME}/Dev/perl/Catmandu
PROJ=${CATMANDU}/examples/aleph

if [ "$1" == "" ]; then
  ${PERL} ${CATMANDU}/bin/catmandu export \
     --pretty \
     -s path=data/aleph.db 
else
  ${PERL} ${CATMANDU}/bin/catmandu export \
     --pretty -l $1 \
     -s path=data/aleph.db 
fi
