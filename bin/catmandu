#!/usr/bin/env perl
use strict;
use warnings;

BEGIN {
    eval "use Catmandu::CLI" or do {
        use File::Spec::Functions qw(catdir);
        use File::Basename qw(dirname);
        use Cwd qw(realpath);
        use lib realpath(catdir(dirname(__FILE__), '..', 'lib'));
        use Catmandu::CLI;
    };
}

Catmandu::CLI->run;
