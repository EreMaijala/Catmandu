#!/usr/bin/env perl
# ABSTRACT: Run Catmandu commands
# PODNAME: catmandu
use strict;
use warnings;

BEGIN {
    eval "use Catmandu::Cmd" or do {
        use Path::Class ();
        use lib Path::Class::file(__FILE__)->dir->parent->subdir('lib')
            ->absolute->resolve->stringify;
        use Catmandu::Cmd;
    };
}

Catmandu::Cmd->run;

